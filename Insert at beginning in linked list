#include<stdio.h>
#include<stdlib.h>
struct Node{                            //              ______>   Saves address of the next node;
int data;                                 //    _______|____
struct Node*next;   // Adress of the next node  |____|____|
};
struct Node*head;
void Insert(int x)
{
struct Node*temp = (struct Node*)malloc(sizeof(struct Node));    //Creating a temperory Node

temp->data=x;    //showing "(*temp).data=x" temp pointing to data is input x;
temp->next=head;  //temperory address value will be stored in head;
head=temp;             
}

void Print()
{
struct Node*temp=head;
printf("List is:\n");
while(temp!=NULL)
{
printf("%d ",temp->data);
temp=temp->next;                //To traverse
}
printf("\n");
}
int main()
{
head=NULL;
int n,i,x;
printf("Enter how many number:\n");
scanf("%d",&n);
printf("Enter the numbers:\n");
for(i=0;i<n;i++)
{
scanf("%d",&x);

Insert(x);
Print();
}
return 0;
}
